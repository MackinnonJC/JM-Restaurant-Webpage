<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="494.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="old.colony.bankaccountapp.DepositController">
   <children>
      <Label layoutX="72.0" layoutY="45.0" prefHeight="18.0" prefWidth="227.0" text="Deposit">
         <font>
            <Font name="Corbel Light" size="23.0" />
         </font>
      </Label>
      <Button fx:id="depositReturn" layoutX="27.0" layoutY="46.0" mnemonicParsing="false" onAction="#returnDeposit" prefHeight="26.0" prefWidth="34.0" text="&lt;" />
      <TextField fx:id="depositAmount" layoutX="108.0" layoutY="130.0" prefHeight="26.0" prefWidth="278.0" />
      <Label alignment="CENTER_RIGHT" layoutX="9.0" layoutY="134.0" prefHeight="18.0" prefWidth="94.0" text="Amount:">
         <font>
            <Font name="Calibri" size="16.0" />
         </font>
      </Label>
      <Button fx:id="depositConfirm" layoutX="196.0" layoutY="187.0" mnemonicParsing="false" onAction="#attemptDeposit" prefHeight="26.0" prefWidth="94.0" text="Confirm" />
      <Button fx:id="logoutButton" layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#logout" text="Log Out" />
      <Label fx:id="depositOutput" alignment="TOP_CENTER" layoutX="73.0" layoutY="224.0" prefHeight="103.0" prefWidth="359.0" style="-fx-border-style: solid; -fx-label-padding: 5px; -fx-border-color: rgb(200, 200, 200);" text="Information about the transaction will go here" wrapText="true">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
